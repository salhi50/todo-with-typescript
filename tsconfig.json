{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "removeComments": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noImplicitReturns": true,
    "allowUnreachableCode": false,
    "skipLibCheck": true
  }
}
